<html>

<head>
  <title>Member</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>

<body>

<table>
	<% for user in @users %>
                <tr>
                <td><%= h user.id %></td>
                
                <td><%= h user.email %></td>
                <td><%= h user.created_at %></td>
                <td>
                    <button> bann </button>
                </td>
                        
        </tr>
    <% end %>
</table>

<% if user_signed_in? %>
    <% if current_user.stall? %>
    <div class='ablock'>
      <p>
        <%= link_to "Ratings & Reviews History", dishes_path %>
      </p>
      </div>
      <% end %>
<% end %>


</body>
</html>
